{
    "name": "tataru-helper-node",
    "version": "2.4.0",
    "description": "tataru-helper-node",
    "main": "src/main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "repository": "https://github.com/winw1010/tataru-helper-node-v2",
    "keywords": [],
    "author": "winw1010",
    "license": "MIT",
    "devDependencies": {
        "electron": "^26.2.4",
        "electron-builder": "^24.6.4",
        "eslint": "^8.50.0"
    },
    "dependencies": {
        "@google-cloud/vision": "^4.0.2",
        "axios": "^1.5.1",
        "crypto-js": "^4.1.1",
        "download": "^8.0.0",
        "sharp": "^0.32.6",
        "temp": "^0.9.4",
        "tesseract.js": "^5.0.0"
    },
    "build": {
        "appId": "com.app.tataru.helper.node",
        "productName": "Tataru Helper Node",
        "directories": {
            "output": "build"
        },
        "extraFiles": [
            "actions-latest.json",
            "signatures-global-latest.json",
            "signatures.json",
            "statuses-latest.json",
            "structures-global-latest.json",
            "zones-latest.json",
            "src/data",
            "!src/data/img/*.png"
        ],
        "win": {
            "requestedExecutionLevel": "highestAvailable",
            "icon": "src/html/img/icon/tataru.ico",
            "target": "nsis"
        },
        "nsis": {
            "createStartMenuShortcut": true,
            "createDesktopShortcut": "always"
        }
    }
}
